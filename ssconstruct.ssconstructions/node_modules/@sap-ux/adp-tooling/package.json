    "@sap-ux/store": "1.1.4"
  },
  "devDependencies": {
    "@types/adm-zip": "0.5.5",
    "@types/ejs": "3.1.2",
    "@types/express": "4.17.21",
    "@types/inquirer": "8.2.6",
    "@types/mem-fs": "1.1.2",
    "@types/mem-fs-editor": "7.0.1",
    "@types/prompts": "2.4.4",
    "@types/supertest": "2.0.12",
    "@types/uuid": "10.0.0",
    "dotenv": "16.3.1",
    "express": "4.21.2",
    "nock": "13.4.0",
    "rimraf": "5.0.5",
    "supertest": "7.1.4",
    "cross-env": "^7.0.3",
    "@sap-ux/store": "1.1.4"
  },
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --watch",
    "clean": "rimraf --glob dist test/test-output coverage *.tsbuildinfo",
    "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../.prettierignore",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "cross-env FIORI_TOOLS_DISABLE_SECURE_STORE=true jest --ci --forceExit --detectOpenHandles --colors --testPathPatterns=test/unit",
    "test-u": "cross-env FIORI_TOOLS_DISABLE_SECURE_STORE=true jest --ci --forceExit --detectOpenHandles --colors -u",
    "link": "pnpm link --global",
    "unlink": "pnpm unlink --global"
  }
}